{
  "name": "saml-assertion",
  "version": "1.0.0",
  "description": "Utility for generating SAML assertions",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "lint": "eslint 'src/**/*!(*.d.ts|*.json)' --cache",
    "format": "yarn prettier 'src/**/*.{ts,json}' --write --loglevel warn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamolins/saml-assertion.git"
  },
  "keywords": [],
  "author": "Kārlis Amoliņš",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamolins/saml-assertion/issues"
  },
  "eslintConfig": {
    "extends": [
      "twilio-ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn format && git add ."
    }
  },
  "homepage": "https://github.com/kamolins/saml-assertion#readme",
  "devDependencies": {
    "@types/node": "^14.11.8",
    "eslint": "^7.11.0",
    "eslint-config-twilio-ts": "^1.29.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
