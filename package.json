{
  "name": "saml-assertion-generator",
  "version": "1.0.0",
  "description": "SAML Assertion Generator",
  "npmClient": "yarn",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "lint": "eslint 'src/**/*!(*.d.ts|*.json)' --cache",
    "format": "yarn prettier 'src/**/*.{ts,json}' --write --loglevel warn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamolins/saml-assertion-generator.git"
  },
  "keywords": [],
  "author": "Kārlis Amoliņš",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamolins/saml-assertion-generator/issues"
  },
  "eslintConfig": {
    "extends": [
      "twilio-ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/lint-staged"
    }
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "homepage": "https://github.com/kamolins/saml-assertion-generator#readme",
  "devDependencies": {
    "@types/node": "^14.11.8",
    "eslint": "^7.11.0",
    "eslint-config-twilio-ts": "^1.29.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
